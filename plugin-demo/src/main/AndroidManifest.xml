<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.chenhao.plugin.demo">

    <application
        android:allowBackup="true"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <!-- 插件Id，必须 -->
        <meta-data
            android:name="PluginId"
            android:value="id_1" />
        <!-- 最低版本ApiLevel，必须 -->
        <meta-data
            android:name="PluginMinApiLevel"
            android:value="1" />
        <!-- 开发者Id, 必须 -->
        <meta-data
            android:name="PluginDeveloperId"
            android:value="id_894148746" />
        <!-- 消息处理入口，必须 -->
        <meta-data
            android:name="PluginMessageReceiver"
            android:value="com.chenhao.plugin.demo.MessageReceiver" />

    </application>

</manifest>
